<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shop.data.mapper.daishu.ScheduleMapper">
	<update id="update">
		update t_schedule set ${name}=#{value} where id=1
	</update>
	<select id="selectById" resultType="Schedule">
		select * from t_schedule where id=1
	</select>
	<select id="countOrderByServiceDateAndTimeType" resultType="int">
		SELECT count(*) FROM t_order where baojie_type = #{baoJieType} and service_date=#{serviceDate} and service_time_type = #{serviceTimeType} and ayi_id is not null
	</select>
</mapper>